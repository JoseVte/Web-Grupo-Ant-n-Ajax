
{% extends app.request.isXmlHttpRequest ? "DSSProyectoBundle::layout_ajax.html.twig": "DSSProyectoBundle::plantilla.html.twig" %}

{% block content %}

    <h1>Panel de control</h1>

    <br>Cambiar email
    <br>Cambiar pass
    <br><a class="{{ajax_div_flotante.class}}" href ="{{ path('dss_proyecto_gestion_eliminar', { 'tabla': 'usuario','nif': app.security.getToken().getUser().getNif() }) }}">Borrar usuario</a>

    {% if (app.security.getToken().getUser().getNvAcceso() == 'ROLE_ADMIN') %}
        <br><a class="{{ajax_div_flotante.class}}" href ="{{ path('dss_proyecto_gestion_crear', { 'tabla': 'usuario'})}}" >Crear usuario</a>
    {% endif %}
{% endblock %}